# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_SERVICE_KEY=your-service-key-here

# Frontend Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Eleanor LLM Configuration (optional - keep local)
VITE_ELEANOR_API_URL=http://localhost:8001
MODEL_PATH=./models/Eleanor
CUDA_VISIBLE_DEVICES=0

# ElevenLabs Voice Configuration
VITE_ELEVENLABS_API_KEY=your-elevenlabs-api-key

# Google OAuth
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Development Settings
VITE_ENABLE_PROFILE_SYNC=true
NODE_ENV=development

# Production Settings
# For Vercel deployment, set NODE_ENV=production in build settings
# Set VITE_ELEANOR_API_URL=disabled to disable Eleanor API in production